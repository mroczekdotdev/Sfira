@model IEnumerable<PostViewModel>

<section class="Feed PostsFeed">
    @if (Model.Any())
    {
        @foreach (var post in Model)
        {
            await Html.RenderPartialAsync("_PostPartial", post);
        }
    }
    else
    {
        <section class="Info">
            <header>User doesn’t have any posts</header>
        </section>
    }
</section>
